package com.entity;

public class Login {

	private int accNo;
	private int password;
	public Login(int accNo, int password) {
		super();
		this.accNo = accNo;
		this.password = password;
	}
	public int getAccNo() {
		return accNo;
	}
	public void setAccNo(int accNo) {
		this.accNo = accNo;
	}
	public int getPassword() {
		return password;
	}
	public void setPassword(int password) {
		this.password = password;
	}
	
}
